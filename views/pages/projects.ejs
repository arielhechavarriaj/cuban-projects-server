<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>Projects</h1>

        <ul>
            <% projects.forEach(function(project) { %>
                <li>
                    <strong><%= project.name %></strong><br>
                    <strong>Name:</strong> <%= project.name %> <br>  <strong>Description:</strong> <%= project.description %> <br> 
                    <strong>Url:</strong> <%= project.url %>
                </li>
            <% }); %>
        </ul>

     </div>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>